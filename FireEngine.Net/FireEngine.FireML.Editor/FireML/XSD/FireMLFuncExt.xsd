          <xs:documentation>最新的函数哦</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:attribute name="return" type="VarRef">
            <xs:annotation>
              <xs:documentation>将返回值保存到某个变量中</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
      <xs:element name="MyFunc" nillable="true">
